public class TesteArrayUtil {

	public static void main(String[] args) {
		ArrayList lista = new ArrayList();
		
		Conta cc = new ContaCorrente(12, 123);
        lista.add(cc);

        Conta cc2 = new ContaCorrente(34, 345);
        lista.add(cc2);
	}

}
